{
  "routes": [
    {
      "route": "/authentication/login-callback",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/authentication/logout-callback",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/authentication/login-failed",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/authentication/logout-failed",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/debug",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/service-worker.js",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      },
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/service-worker.published.js",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      },
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/service-worker-assets.js",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      },
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/manifest.webmanifest",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      },
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,ico}", 
      "/css/*", 
      "/js/*", 
      "/*.{js,css,woff,woff2,ttf,svg,ico,png,jpg,gif,json}",
      "/_framework/*",
      "/api/*",
      "/service-worker*.js"
    ]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/authentication/login",
      "statusCode": 302
    },
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "cache-control": "must-revalidate, max-age=3600"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm",
    ".dat": "application/octet-stream",
    ".dll": "application/octet-stream",
    ".blat": "application/octet-stream",
    ".br": "application/x-brotli",
    ".gz": "application/gzip"
  }
}