{
  "routes": [
    {
      "route": "/authentication/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/authentication/login",
      "statusCode": 302
    },
    "403": {
      "redirect": "/authentication/login",
      "statusCode": 302
    },
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/_framework/*", "/_content/*"]
  },
  "mimeTypes": {
    ".webmanifest": "application/manifest+json"
  }
}