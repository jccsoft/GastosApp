{
  "routes": [
    {
      "route": "/authentication/login-callback*",
      "rewrite": "/index.html"
    },
    {
      "route": "/authentication/logout-callback*",
      "rewrite": "/index.html"
    },
    {
      "route": "/authentication/login-failed*",
      "rewrite": "/index.html"
    },
    {
      "route": "/authentication/logout-failed*",
      "rewrite": "/index.html"
    },
    {
      "route": "/authentication/*",
      "rewrite": "/index.html"
    },
    {
      "route": "/debug-auth",
      "rewrite": "/index.html"
    },
    {
      "route": "/pwa-settings",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/authentication/login",
      "statusCode": 302
    },
    "403": {
      "redirect": "/authentication/login",
      "statusCode": 302
    },
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/_framework/*", "/_content/*", "/lib/*", "/*.{ico,png,jpg,gif,svg,webmanifest}"]
  },
  "mimeTypes": {
    ".webmanifest": "application/manifest+json"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate",
    "Pragma": "no-cache",
    "Expires": "0"
  }
}